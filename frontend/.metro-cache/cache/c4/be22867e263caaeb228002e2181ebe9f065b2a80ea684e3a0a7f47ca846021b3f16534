rds.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getLoginRewardsStatus(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/login-rewards/${encodeURIComponent(u)}`);\n    return res.data;\n  }\n  async function claimLoginReward(username, day) {\n    const u = requireUsername(username);\n    const res = await api.post(`/login-rewards/${encodeURIComponent(u)}/claim/${day}`, null);\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // JOURNEY / DAILY TRACK (journey.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getJourneyStatus(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/journey/status/${encodeURIComponent(u)}`);\n    return res.data;\n  }\n  async function claimJourneyDaily(username, day) {\n    const u = requireUsername(username);\n    const res = await api.post(`/journey/claim-daily/${encodeURIComponent(u)}`, null, {\n      params: {\n        day\n      }\n    });\n    return res.data;\n  }\n  async function claimJourneyMilestone(username, milestoneId) {\n    const u = requireUsername(username);\n    const res = await api.post(`/journey/claim-milestone/${encodeURIComponent(u)}`, null, {\n      params: {\n        milestone_id: milestoneId\n      }\n    });\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // CAMPAIGN / STORY (campaign.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getCampaignChapters(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/campaign/chapters`, {\n      params: {\n        username: u\n      }\n    });\n    return res.data;\n  }\n  async function getCampaignChapterDetail(username, chapterId) {\n    const u = requireUsername(username);\n    const res = await api.get(`/campaign/chapter/${encodeURICo