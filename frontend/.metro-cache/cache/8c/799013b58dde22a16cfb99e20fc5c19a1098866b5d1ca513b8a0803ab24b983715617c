\n      }\n    });\n    return res.data;\n  }\n  async function unblockChatUser(blockedUsername) {\n    const res = await api.post(`/chat/unblock-user`, null, {\n      params: {\n        blocked_username: blockedUsername\n      }\n    });\n    return res.data;\n  }\n  async function getBlockedUsers() {\n    const res = await api.get(`/chat/blocked-users`);\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // GUILD (guild.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getGuild(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/guild/${encodeURIComponent(u)}`);\n    return res.data;\n  }\n  async function getGuildLevelInfo(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/guild/${encodeURIComponent(u)}/level-info`);\n    return res.data;\n  }\n  async function getAvailableGuilds(limit = 20) {\n    const res = await api.get(`/guilds`, {\n      params: {\n        limit\n      }\n    });\n    return res.data;\n  }\n  async function createGuildApi(username, guildName) {\n    const u = requireUsername(username);\n    const res = await api.post(`/guild/create`, null, {\n      params: {\n        username: u,\n        guild_name: guildName\n      }\n    });\n    return res.data;\n  }\n  async function joinGuildApi(username, guildId) {\n    const u = requireUsername(username);\n    const res = await api.post(`/guild/join`, null, {\n      params: {\n        username: u,\n        guild_id: guildId\n      }\n    });\n    return res.data;\n  }\n  async function leaveGuildApi(username) {\n    const u = requireUsername(username);\n    const res = await api.post(`/guild/leave`, null, {\n      params: {\n        username: u\n      }\n    });\n    return res.data;\n  }\n  async function getGuildBoss(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/guild/${encodeURIComponent(u)}/boss