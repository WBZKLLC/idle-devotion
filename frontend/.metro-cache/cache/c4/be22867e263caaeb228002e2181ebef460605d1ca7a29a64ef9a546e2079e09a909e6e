onentId, sourceId) {\n    const res = await api.post('/pvp/match', {\n      opponent_id: opponentId,\n      source_id: sourceId\n    });\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // LEADERBOARD (leaderboard.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getLeaderboard(type, limit = 50) {\n    // Map type names to backend endpoint paths\n    const typeEndpoints = {\n      'arena': '/leaderboard/arena',\n      'cr': '/leaderboard/cr',\n      'power': '/leaderboard/power',\n      'abyss': '/leaderboard/abyss',\n      'campaign': '/leaderboard/campaign'\n    };\n    const endpoint = typeEndpoints[type] || `/leaderboard/${encodeURIComponent(type)}`;\n    const res = await api.get(endpoint, {\n      params: {\n        limit\n      }\n    });\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // ABYSS (abyss.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getAbyssStatus(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/abyss/${encodeURIComponent(u)}/status`);\n    return res.data;\n  }\n  async function getAbyssLeaderboard(serverId, limit = 50) {\n    const res = await api.get(`/abyss/leaderboard/${encodeURIComponent(serverId)}`, {\n      params: {\n        limit\n      }\n    });\n    return res.data;\n  }\n  async function getAbyssRecords(username, level) {\n    const u = requireUsername(username);\n    const res = await api.get(`/abyss/${encodeURIComponent(u)}/records`, {\n      params: {\n        level\n      }\n    });\n    return res.data;\n  }\n  async function attackAbyss(username, level) {\n    const u = requireUsername(username);\n    const res = await api.post(`/abyss/${encodeURIComponent(u)}/attack`, null, {\n      params: level ? {\n        level\n      } : {}\n    });\n    return res.data;\n  }\n\n  // ──────────────────────────