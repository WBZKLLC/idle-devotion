mat\n    const data = res.data;\n    return {\n      time_elapsed: data.elapsedSeconds ?? 0,\n      max_hours: data.capHours ?? 168,\n      gold_pending: data.pendingRewards?.gold ?? 0,\n      is_capped: data.isCapped ?? false,\n      // Keep original data for reference\n      ...data\n    };\n  }\n  async function instantCollectIdle(username) {\n    const u = requireUsername(username);\n    const res = await api.post(`/idle/instant-collect/${encodeURIComponent(u)}`);\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // HERO UPGRADE (hero-upgrade.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getHeroDetails(heroId, username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/hero/${encodeURIComponent(heroId)}/details`, {\n      params: {\n        username: u\n      }\n    });\n    return res.data;\n  }\n  async function levelUpHero(heroId, username, levels = 1) {\n    const u = requireUsername(username);\n    const res = await api.post(`/hero/${encodeURIComponent(heroId)}/level-up`, null, {\n      params: {\n        username: u,\n        levels\n      }\n    });\n    return res.data;\n  }\n  async function awakenHero(heroId, username) {\n    const u = requireUsername(username);\n    const res = await api.post(`/hero/${encodeURIComponent(heroId)}/awaken`, null, {\n      params: {\n        username: u\n      }\n    });\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // HERO MANAGER (hero-manager.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getTeamsByMode(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/team/${encodeURIComponent(u)}/by-mode`);\n    return res.data;\n  }\n  async function getTeamsList(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/team/${encodeURIComponent(u)