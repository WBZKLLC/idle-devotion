 response\n        setLocalHeroOverride(prev => Object.assign({}, prev, {\n          stars: newStars,\n          duplicates: remaining\n        }));\n        await fetchUser();\n        // Refresh store hero (this will update storeHero via selector)\n        await getUserHeroById(String(heroId), {\n          forceRefresh: true\n        });\n\n        // Clear local override now that store is fresh\n        setLocalHeroOverride(null);\n\n        // Compute newTier using authoritative server values\n        const newTier = (0, _libProgression.unlockedTierForHero)(Object.assign({}, optimisticHero, {\n          stars: newStars,\n          duplicates: remaining\n        }));\n        if (newTier > effectiveUnlockedTier) {\n          // Phase 3.19.9: In-app reward recap modal\n          setRewardRecap({\n            title: 'Star Promoted! ðŸŒŸ',\n            message: `New tier art unlocked: ${(0, _libProgression.labelForTier)(newTier)}\\n\\nReturn to Hero Detail to preview your new art!`,\n            tone: 'gold',\n            buttonText: 'Awesome!'\n          });\n        } else {\n          _componentsUiToast.toast.success(`Star Promoted! Now at ${newStars} star(s).`);\n        }\n      } catch (e) {\n        // Rollback to canonical pre-optimistic hero state\n        if (rollbackRef.current?.hero) setLocalHeroOverride(rollbackRef.current.hero);\n        if (!(0, _libApi.isErrorHandledGlobally)(e)) {\n          _componentsUiToast.toast.error(e?.response?.data?.detail || 'Unable to promote this hero right now.');\n        }\n      } finally {\n        rollbackRef.current = null;\n        setIsPromoting(false);\n      }\n    }, [hero, storeHero, heroId, isMaxStars, nextStar, shards, shardsNeededForNext, _libProgression.unlockedTierForHero, effectiveUnlockedTier, user?.username, fetchUser, fetchUserHeroes]);\n\n    // --- Ascension: UI-only placeholder (endpoint may not exist yet) ---\n    const tryAscend = (0, _react.useCallback)(async () => {\n      setShowConfirmAscend