ames`);\n    return res.data;\n  }\n  async function equipFrameApi(username, frameId) {\n    const u = requireUsername(username);\n    const res = await api.post(`/user/${encodeURIComponent(u)}/equip-frame`, null, {\n      params: {\n        frame_id: frameId\n      }\n    });\n    return res.data;\n  }\n  async function unequipFrameApi(username) {\n    const u = requireUsername(username);\n    const res = await api.post(`/user/${encodeURIComponent(u)}/unequip-frame`);\n    return res.data;\n  }\n  async function redeemCode(username, code) {\n    const u = requireUsername(username);\n    const res = await api.post(`/codes/redeem`, null, {\n      params: {\n        username: u,\n        code\n      }\n    });\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // LAUNCH BANNER (launch-banner.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getLaunchBannerStatus(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/launch-banner/status/${encodeURIComponent(u)}`);\n    return res.data;\n  }\n  async function pullLaunchBanner(username, multi = false) {\n    const u = requireUsername(username);\n    const res = await api.post(`/launch-banner/pull/${encodeURIComponent(u)}`, null, {\n      params: {\n        multi\n      }\n    });\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // LOGIN REWARDS (login-rewards.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getLoginRewardsStatus(username) {\n    const u = requireUsername(username);\n    const res = await api.get(`/login-rewards/${encodeURIComponent(u)}`);\n    return res.data;\n  }\n  async function claimLoginReward(username, day) {\n    const u = requireUsername(username);\n    const res = await api.post(`/login-rewards/${encodeURIComponent(u)}/claim/${day}`, null);\n    return res.data;\n  }\n\n  // ───