ync function enhanceEquipment(username, equipmentId, materials) {\n    const u = requireUsername(username);\n    const res = await api.post(`/equipment/${encodeURIComponent(u)}/enhance`, {\n      equipment_id: equipmentId,\n      materials\n    });\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // EVENTS / EVENT BANNERS (events.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function getEventBanners() {\n    const res = await api.get(`/event-banners`);\n    return res.data;\n  }\n  async function pullEventBanner(username, bannerId, multi = false) {\n    const u = requireUsername(username);\n    const res = await api.post(`/event-banners/${encodeURIComponent(bannerId)}/pull`, null, {\n      params: {\n        username: u,\n        multi\n      }\n    });\n    return res.data;\n  }\n\n  // ─────────────────────────────────────────────────────────────\n  // ADMIN (admin.tsx)\n  // ─────────────────────────────────────────────────────────────\n\n  async function adminGetUser(usernameOrId) {\n    const q = requireUsername(usernameOrId);\n    const res = await api.get(`/user/${encodeURIComponent(q)}`);\n    return res.data;\n  }\n  async function adminGrantResources(targetUsername, adminKey, resources) {\n    const res = await api.post(`/admin/grant-resources/${encodeURIComponent(targetUsername)}`, {\n      resources\n    }, {\n      params: {\n        admin_key: adminKey\n      }\n    });\n    return res.data;\n  }\n  async function adminSetVIP(targetUsername, adminKey, vipLevel) {\n    const res = await api.post(`/admin/set-vip/${encodeURIComponent(targetUsername)}`, null, {\n      params: {\n        admin_key: adminKey,\n        vip_level: vipLevel\n      }\n    });\n    return res.data;\n  }\n  async function adminMuteUser(targetUsername, adminKey, durationHours) {\n    const res = await api.post(`/admin/mute-user/${encodeURIComponent(targetUsername)}`, null, {\n      params: {\n 